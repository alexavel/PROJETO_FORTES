  object GravaDiscDet: TFDQuery
    Connection = fdConexao
    SQL.Strings = (
      'insert into disciplina_det (dpd_id, dpd_iddcp, dpd_idpfr )'
      
        '                    values ((Select Coalesce( max(dpd_id),0)+1 F' +
